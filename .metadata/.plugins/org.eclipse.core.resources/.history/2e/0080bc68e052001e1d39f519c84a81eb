package AnnotationBased.Configuration;

import org.springframework.stereotype.Component;

@Component
public class Car implements Vehicle{

	
	private Tyre tyre;
	
	public Tyre getTyre() {
		return tyre;
	}

	public void setTyre(Tyre tyre) {
		this.tyre = tyre;
	}

	@Override
	public String toString() {
		return "Tyre";
	}

	public void drive() {
		System.out.println("Driving...");
	}
	
}
